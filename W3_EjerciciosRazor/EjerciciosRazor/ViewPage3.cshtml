@{
    Random numero = new Random();

    var numeros = numero.Next(1,10);
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <table>
            @for (int i = 0; i < @numeros; i++)
            {
                <tr>
                    <td>
                        Cuadro
                    </td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
